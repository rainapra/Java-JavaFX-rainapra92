<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="408.0" prefWidth="437.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.AdminController">
   <children>
      <Button fx:id="addMovie" layoutX="67.0" layoutY="106.0" mnemonicParsing="false" onAction="#directMovie" text="Add movie" textFill="#5417c6">
         <font>
            <Font name="Berlin Sans FB Demi Bold" size="15.0" />
         </font></Button>
      <Button fx:id="deleteMovie" layoutX="67.0" layoutY="168.0" mnemonicParsing="false" prefHeight="26.0" prefWidth="111.0" text="Delete movie" textFill="#0f0fe1">
         <font>
            <Font name="Berlin Sans FB Demi Bold" size="14.0" />
         </font></Button>
      <Button fx:id="updateMovie" layoutX="67.0" layoutY="230.0" mnemonicParsing="false" text="Update movie" />
      <Button fx:id="logout" layoutX="184.0" layoutY="316.0" mnemonicParsing="false" onAction="#logout" text="Log out" textFill="#3d12da">
         <font>
            <Font name="Berlin Sans FB Demi Bold" size="15.0" />
         </font></Button>
   </children>
</AnchorPane>
